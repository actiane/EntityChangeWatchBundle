{
  "name": "actiane/entity-change-watch-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Allows you to listen to the life cycle of your entities",
  "autoload": {
    "psr-4": {
      "Actiane\\EntityChangeWatchBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Actiane\\EntityChangeWatchBundle\\Tests\\": "Tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "^4.1",
    "doctrine/orm": "~2.4,>=2.4.5",
    "doctrine/doctrine-bundle": "~1.4",
    "symfony/property-access": "~4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.1",
    "symfony/yaml": "~4.1",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "minimum-stability": "dev"
}
