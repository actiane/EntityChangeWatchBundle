{
  "name": "actiane/entity-change-watch-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Allows you to listen to the life cycle of your entities",
  "autoload": {
    "psr-4": {
      "Actiane\\EntityChangeWatchBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Actiane\\EntityChangeWatchBundle\\Tests\\": "Tests/"
    }
  },
  "require": {
    "ext-pdo_sqlite": "*",
    "php": ">7.4",
    "symfony/framework-bundle": "^4.1|^5.1",
    "doctrine/orm": "^2.7.0",
    "doctrine/doctrine-bundle": "^2.0",
    "symfony/property-access": "^4.1|^5.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.8",
    "symfony/yaml": "^5.0",
    "php-coveralls/php-coveralls": "^2.4.3",
    "doctrine/annotations": "^1.3"
  },
  "minimum-stability": "stable"
}
