{
  "name": "actiane/entity-change-watch-bundle",
  "license": "MIT",
  "type": "symfony-bundle",
  "description": "Allows you to listen to the life cycle of your entities",
  "autoload": {
    "psr-4": {
      "Actiane\\EntityChangeWatchBundle\\": ""
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Actiane\\EntityChangeWatchBundle\\Tests\\": "Tests/"
    }
  },
  "require": {
    "php": "^7.1",
    "symfony/framework-bundle": "^5.3.4",
    "doctrine/orm": "^2.7.0",
    "doctrine/doctrine-bundle": "~2.4.2",
    "symfony/property-access": "^v5.3.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5.8",
    "symfony/yaml": "^5.3.4",
    "php-coveralls/php-coveralls": "^2.4.3",
    "doctrine/annotations": "1.14.x-dev"
  },
  "minimum-stability": "dev"
}
